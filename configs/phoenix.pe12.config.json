[

    {
        "kernelSource": {
            "name": "k30p",
            "repo": "https://github.com/liyafe1997/kernel_xiaomi_sm8250_mod",
            "branch": "android14-stable-mod",
            "device": "lmi",
            "defconfig": "lmi_defconfig"
          },
          "toolchains": [
          {
            "repo": "https://gitlab.com/ThankYouMario/android_prebuilts_clang-standalone/",
            "branch": "18",
            "name": "clang",
            "binaryEnv": [
                "bin"
            ]
          },
          {
            "repo": "https://android.googlesource.com/platform/prebuilts/gas/linux-x86",
            "branch": "main",
            "name": "gas",
            "binaryEnv": [
                "bin"
            ]
          }
        ],
          "enableCcache": true,
        "params": {
          "ARCH": "arm64",
          "CC": "clang",
          "externalCommands": {
            "CLANG_TRIPLE": "aarch64-linux-gnu-",
            "CROSS_COMPILE": "aarch64-linux-gnu-",
            "CROSS_COMPILE_ARM32": "arm-linux-gnueabi-",
            "CROSS_COMPILE_COMPAT": "arm-linux-gnueabi-"
          }
          },
          "AnyKernel3": {
            "use": true,
            "release": true,
            "custom": {
            "repo": "https://github.com/luyanci/AnyKernel3",
            "branch": "master"
        }
          },
          "enableKernelSU": true,
          "enableLXC": false
    }
]
